 <h1>
   Listing positions

   <%= form_tag positions_path, method: :get, :class => 'float-right form-inline' do %>
     <%= text_field_tag :term, params[:term], autofocus: true, :class => 'form-control mr-sm-2', :type => 'search', :placeholder => 'Search', autocomplete: 'off' %>
     <%= submit_tag 'Search', nombre: nil, :class => 'btn btn-outline-success my-2 my-sm-0' %>&nbsp&nbsp&nbsp&nbsp
     <%= link_to "", position_new_path, class: 'btn btn-primary fas fa-plus rounded-1 float-right form-inline' %>
   <% end %>

 </h1>
<br>
<br>
<table class="table table-striped table-hover">
  <thead>
  <tr>
    <th>Position</th>
    <th>Options</th>
  </tr>
  </tr>
  </thead>

  <% @get_position_list.each do |position| %>
    <tbody>
    <tr>
      <th><%= position.position_name %></th>
      <td>
        &nbsp&nbsp<%= link_to "", position_edit_path(position),  class:'btn btn-outline rounded-0 fas fa-pencil-alt fa-2x' %>&nbsp&nbsp
      </td>
    </tr>
    </tbody>
  <% end %>
</table>

 <div class="apple_pagination">
   <div class="page_info">
     <%= page_entries_info @get_position_list %>
   </div>
   <%= will_paginate @get_position_list, :container => false %>
 </div>