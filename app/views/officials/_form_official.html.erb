<h2>
  <%= title %>
  <%= link_to "", officials_path , class: 'btn btn-secondary fas fa-undo-alt rounded-1 float-right form-inline' %>
</h2>

<%= form_for @official do |f| %>
  <div class="container">
    <div class="row mt-3">
      <div class="col">
        <form action="">
          <div class="row justify-content-center">
            <div class="col-12 col-sm-9 col-md-4">
              <br>
              <label><b>Proveedor</b></label>
              <%= f.text_field :first_name, :type => 'text', :class => 'form-control', :style=>"text-transform:uppercase", :placeholder => 'Ingrese proveedor', autocomplete: 'off', required: true, autofocus: true %>
              <br>
              <label><b>Producto</b></label>
              <%= f.text_field :last_name, :type => 'text', :class => 'form-control', :style=>"text-transform:uppercase",:placeholder => 'Ingrese producto', autocomplete: 'off', required: true %>
              <br>
              <label><b>Descripcion</b></label>
              <%= f.text_field :address, :type => 'text', :class => 'form-control', :style=>"text-transform:uppercase",:placeholder => 'Ingrese descripcion', autocomplete: 'off', required: true %>
              <br>
              <label><b>Precio Unitario</b></label>
              <%= f.number_field :document_number, :type => 'number', :step=>"0.001", :class => 'form-control', :style=>"text-transform:uppercase", :placeholder => 'Ingrese precio', autofocus: true, autocomplete: 'off', required: true %>
              <br>
            </div>
          </div>

          <div class="form-group row">
            <div class="col-12 text-center">
              <div class="row justify-content-center">
                <div class="col-12 col-sm-9 col-md-4">
                  <%= f.submit :Crear, :class => 'btn btn-primary btn-block' %>
                </div>
              </div>
            </div>
          </div>

        </form>
      </div>
    </div>
  </div>



<% end %>